journal = {Statistical Mechanics and Scientific Explanation},
keywords = {second law of thermodynamics,statistical mechanics,thermal equi-},
pages = {519--581},
title = {{Gibbs and Boltzmann Entropy in Classical and Quantum Mechanics}},
year = {2020}
}
