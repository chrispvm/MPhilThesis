@article{Betancur1993,
abstract = {Between 1980 and 1991, the Republic of El Salvador in Central America$\backslash$nwas engulfed in a war which plunged Salvadorian society into violence,$\backslash$nleft it with thousands and thousands of people dead and exposed it$\backslash$nto appalling crimes, until the day - 16 January 1992 - when the parties,$\backslash$nreconciled, signed the Peace Agreement in the Castle of Chapultepec,$\backslash$nMexico, and brought back the light and the chance to re-emerge from$\backslash$nmadness to hope.$\backslash$n$\backslash$n$\backslash$nA. INSTITUTIONS AND NAMES$\backslash$n$\backslash$n$\backslash$nViolence was a fire which swept over the fields of El Salvador; it$\backslash$nburst into villages, cut off roads and destroyed highways and bridges,$\backslash$nenergy sources and transmission lines; it reached the cities and$\backslash$nentered families, sacred areas and educational centres; it struck$\backslash$nat justice and filled the public administration with victims; and$\backslash$nit singled out as an enemy anyone who was not on the list of friends.$\backslash$nViolence turned everything to death and destruction, for such is$\backslash$nthe senselessness of that breach of the calm plenitude which accompanies$\backslash$nthe rule of law, the essential nature of violence being suddenly$\backslash$nor gradually to alter the certainty which the law nurtures in human$\backslash$nbeings when this change does not take place through the normal mechanisms$\backslash$nof the rule of law. The victims were Salvadorians and foreigners$\backslash$nof all backgrounds and all social and economic classes, for in its$\backslash$nblind cruelty violence leaves everyone equally defenceless.$\backslash$n$\backslash$n$\backslash$nWhen there came pause for thought, Salvadorians put their hands to$\backslash$ntheir hearts and felt them pound with joy. No one was winning the$\backslash$nwar, everyone was losing it. Governments of friendly countries and$\backslash$norganizations the world over that had looked on in anguish at the$\backslash$ntragic events in that Central American country which, although small,$\backslash$nwas made great by the creativity of its people -all contributed their$\backslash$nideas to the process of reflection. A visionary, Javier P{\'{e}}rez de$\backslash$nCu{\'{e}}llar, then Secretary-General of the United Nations, heeded the$\backslash$nunanimous outcry and answered it. The Presidents of Colombia, Mexico,$\backslash$nSpain and Venezuela supported him. The Chapultepec Agreement expressed$\backslash$nthe support of the new Secretary-General, Mr. Boutros Boutros-Ghali,$\backslash$nfor the search for reconciliation.$\backslash$n$\backslash$n$\backslash$nB. THE CREATIVE CONSEQUENCES$\backslash$n$\backslash$n$\backslash$nOn the long road of the peace negotiations, the need to reach agreement$\backslash$non a Commission on the Truth arose from the Parties recognition that$\backslash$nthe communism which had encouraged one side had collapsed, and perhaps$\backslash$nalso from the disillusionment of the Power which had encouraged the$\backslash$nother. It emerged as a link in the chain of reflection and agreement$\backslash$nand was motivated, ultimately, by the impact of events on Salvadorian$\backslash$nsociety, which now faced the urgent task of confronting the issue$\backslash$nof the widespread, institutionalized impunity which had struck at$\backslash$nits very heart: under the protection of State bodies but outside$\backslash$nthe law, repeated human rights violations had been committed by members$\backslash$nof the armed forces; these same rights had also been violated by$\backslash$nmembers of the guerrilla forces.$\backslash$n$\backslash$n$\backslash$nIn response to this situation, the negotiators agreed that such repugnant$\backslash$nacts should be referred to a Commission on the Truth, which was the$\backslash$nname they agreed to give it from the outset. Unlike the Ad Hoc Commission,$\backslash$nso named because there was no agreement on what to call the body$\backslash$ncreated to purify the armed forces, the Commission on the Truth was$\backslash$nso named because its very purpose and function were to seek, find$\backslash$nand publicize the truth about the acts of violence committed by both$\backslash$nsides during the war.$\backslash$n$\backslash$n$\backslash$nThe truth, the whole truth and nothing but the truth, as the oath$\backslash$ngoes. The overall truth and the specific truth, the radiant but quiet$\backslash$ntruth. The whole and its parts, in other words, the bright light$\backslash$nshone onto a surface to illuminate it and the parts of this same$\backslash$nsurface lit up case by case, regardless of the identity of the perpetrators,$\backslash$nalways in the search for lessons that would contribute to reconciliation$\backslash$nand to abolishing such patterns of behaviour in the new society.$\backslash$n$\backslash$n$\backslash$nLearning the truth and strengthening and tempering the determination$\backslash$nto find it out; putting an end to impunity and cover-up; settling$\backslash$npolitical and social differences by means of agreement instead of$\backslash$nviolent action: these are the creative consequences of an analytical$\backslash$nsearch for the truth.},
author = {Betancur, Belisario and Planchart, Reinaldo Figueredo and Buergenthal, Thomas},
file = {:C$\backslash$:/Users/chris/Documents/Mendeley Desktop/Betancur, Planchart, Buergenthal/Betancur, Planchart, Buergenthal - 1993 - From Madness to Hope the 12-year war in El Salvador Report of the Commission on the Truth for.pdf:pdf},
journal = {The Commission on the Truth for El Salvador.},
pmid = {1029285},
title = {{From Madness to Hope: the 12-year war in El Salvador: Report of the Commission on the Truth for El Salvador}},
year = {1993}
}
