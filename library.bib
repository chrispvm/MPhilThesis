@unpublished{Paries2011a,
abstract = {The financial crisis clearly illuminated the potential$\backslash$namplifying role of financial factors on macroeconomic$\backslash$ndevelopments. Indeed, the heavy impairments of banks� balance$\backslash$nsheets brought to the fore the banking sector�s ability to$\backslash$nprovide a smooth flow of credit to the real economy. However,$\backslash$nmost existing structural macroeconomic models fail to take into$\backslash$naccount the crucial role of banks� balance sheet adjustment in$\backslash$nthe propagation of shocks to the economy. This paper contributes$\backslash$nto fill this gap, analyzing the role of credit market frictions$\backslash$nin business cycle fluctuations and in the transmission of$\backslash$nmonetary policy. We estimate a closed-economy dynamic stochastic$\backslash$ngeneral equilibrium (DSGE) model for the euro area with$\backslash$nfinancially-constrained households and firms and embedding an$\backslash$noligopolistic banking sector facing capital constraints. Using$\backslash$nthis setup we examine the macroeconomic implications of various$\backslash$nfinancial frictions on the supply and demand of credit, and in$\backslash$nparticular we assess the effects of introducing risk-sensitive$\backslash$nand more stringent capital requirements. Finally, we explore the$\backslash$nscope for counter-cyclical bank capital rules and the strategic$\backslash$ncomplementarities between macro-prudential tools and monetary$\backslash$npolicy},
